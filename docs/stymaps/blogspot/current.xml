        } else {
          $("a","#player").attr({"href":"javascript:playerPause()",
                       "style":"font-variant:inherit"})[0].innerHTML = "PAUSE";
        }
      } catch (e) {}
    }, 120);

    setTimeout(function() {
      if (typeof canvasApp[0] !== 'undefined') {
        for(var i=0,n=canvasApp.length;i<n;i++) canvasApp[i]();
      } else {
